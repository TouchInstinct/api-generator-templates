        {%- if field.optional %}
        if {{ field.name }} != nil {
            {{ field.name }} >>> {{ utils.mappingToMapForField(field) }}
        }
        {%- else %}
        {{ field.name }} >>> {{ utils.mappingToMapForField(field) }}
        {%- endif -%}
{%- endfor -%}